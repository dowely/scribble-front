<div class="sub-menu-profile">

  <div class="sub-menu-profile__top-bar">
    <div class="sub-menu-profile__control">
      <div class="top-bar-profile">

        <div class="top-bar-profile__back">

          <div class="top-bar-profile__back-tap-area" id="back-tap-area"></div>
    
          <svg class="top-bar-profile__back-icon">
            <use href="#arrow-left-regular"></use>
          </svg>
    
        </div>
    
        <h2 class="top-bar-profile__title">Profile</h2>

      </div>

    </div>
  </div>

  <div class="sub-menu-profile__progress">
    <div class="sub-menu-profile__control">
      <div class="sub-menu-profile__progress-wrapper">

        <div class="sub-menu-profile__progress-numbers">
          <span>3</span>
          <span>/</span>
          <span>5</span>
        </div>

        <svg class="sub-menu-profile__progress-icon">
          <use href="#award-solid"></use>
        </svg>

      </div>

    </div>
  </div>

  <div class="sub-menu-profile__prompt">
    <div class="sub-menu-profile__control">
      <%- include ../components/sub-menu/_prompt.ejs %>
    </div>
  </div>
