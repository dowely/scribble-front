        <%- include ../components/organizer/_notification-display.ejs %>

      </div>

    </div>

  </div>

  <div class="content__right-col content__right-col--organizer">
    <div class="content__viewer content__viewer--organizer">


      <!-- target = 'right' include ../components/organizer/_calendar-display.ejs -->

      <!-- include ../../../assets/templates/organizer/_item-search.ejs 7449, 9846 3156, 2247, 9244, 8614-->

      <%
      
        let samples = itemServer.combinedItems.filter(item => [9244].some(id => id === item.id))

        samples.forEach(item => { %>

          <%- include ../../../assets/templates/organizer/_item-read.ejs %>

        <% })
        
      %>

    </div>
  </div>

  <div class="content__central-col content__central-col--visible">
    <div class="content__viewer content__viewer--organizer">

      <% let formType = 'note' %>

      <%- include ../../../assets/templates/organizer/_item-form.ejs %>

